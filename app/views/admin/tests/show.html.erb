<h2><%= @test.title %></h2><hr>
  <div>
    <p><%= t('.level') %> <%= @test.level %></p>
    <p><%= t('.category') %> <%= @test.category.title %></p>
  </div>

  <table>
    <th><%= t('.questions') %></th>
    <th colspan="3"><%= t('.actions') %></th>
    <%= render @questions %>
  </table>

<p>
  <%= link_to t('.back'), admin_tests_path %>
  <%= link_to t('.new_question'), new_admin_test_question_path(@test) %>
</p>
